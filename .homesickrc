#!/usr/bin/env ruby

home_directory = ENV['HOME']
original_file = ".zshrc"
pre_file = ".zshrc.pre-oh-my-zsh"

if(File.file?("#{home_directory}/" + "#{pre_file}"))
    File.rename("#{home_directory}/" + "#{pre_file}", "#{home_directory}/" + "#{original_file}")
end

